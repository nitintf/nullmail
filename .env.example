# Redis Configuration
REDIS_PASSWORD=your_secure_redis_password
REDIS_URL=redis://localhost:6379

# SMTP Configuration
SMTP_PORT=:2525

# Domain Configuration
DOMAIN=nullmail.yourdomain.com

# SSL/TLS Configuration (for production)
SSL_CERT_PATH=/path/to/cert.pem
